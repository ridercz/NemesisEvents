{
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "dependencies": {
    "Altairis.NemesisEvents.BL": {
      "target": "project"
    },
    "Altairis.NemesisEvents.DAL": {
      "target": "project"
    },
    "Autofac.Extensions.DependencyInjection": "4.0.0",
    "DotVVM": "1.1.0-alpha-r4",
    "DotVVM.AspNetCore": "1.1.0-alpha-r4",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "1.1.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.AspNetCore.StaticFiles": "1.1.0",
    "Microsoft.EntityFrameworkCore": "1.1.0",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.1.0",
    "Microsoft.NETCore.App": {
      "version": "1.1.0",
      "type": "platform"
    },
    "Riganti.Utils.Infrastructure.AspNetCore": "2.0.0-alpha-06",
    "Riganti.Utils.Infrastructure.AutoMapper": "2.0.0-alpha-06",
    "Riganti.Utils.Infrastructure.DotVVM": "2.0.0-alpha-06",
    "Riganti.Utils.Infrastructure.EntityFrameworkCore": "2.0.0-alpha-06",
    "Riganti.Utils.Infrastructure.Services": "2.0.0-alpha-06",
    "Riganti.Utils.Infrastructure.Services.Amazon.SES": "2.0.0-alpha-06"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config"
    ]
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.1.0-preview4-final",
    "Microsoft.Extensions.SecretManager.Tools": "1.1.0-preview4-final"
  },
  "userSecretsId": "Altairis.NemesisEvents.Web",
  "version": "1.0.0-*"
}
